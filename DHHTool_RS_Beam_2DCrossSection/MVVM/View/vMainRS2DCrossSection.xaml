<Window
    x:Class="DHHTool_RS_Beam_2DCrossSection.MVVM.View.vMainRS2DCrossSection"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:DHHTool_RS_Beam_2DCrossSection.MVVM.View"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Window.Resources>
        <ResourceDictionary Source="pack://application:,,,/DHHLibrary;component/Themes/Themes.xaml"/>
    </Window.Resources>
    <Grid>
        <StackPanel>
            <TextBox
                Style="{DynamicResource TextBoxStyle}"
                Text="RS Beam 2D Cross Section" />
            <Button
                Style="{DynamicResource ButtonStyle}"
                Content="2D Cross Section"
                Command="{Binding OpenRS2DCrossSectionCommand}" />
        </StackPanel>

    </Grid>
</Window>
